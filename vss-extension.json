{
    "manifestVersion": 1,
    "id": "nullplatform-extension",
    "name": "Nullplatform Extension",
    "version": "0.1.23",
    "publisher": "nullplatform",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Extension to integrate with Nullplatform.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "./icons/logo.png"
    },
    "files": [
        {
            "path": "task-nullplatform-login"
        },
        {
            "path": "task-nullplatform-metadata"
        },
        {
            "path": "task-nullplatform-build"
        }
    ],
    "contributions": [
        {
            "id": "task-nullplatform-login",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task-nullplatform-login"
            }
        },
        {
            "id": "task-nullplatform-metadata",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task-nullplatform-metadata"
            }
        },
        {
            "id": "task-nullplatform-build",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task-nullplatform-build"
            }
        }
    ]
}
